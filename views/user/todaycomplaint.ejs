<%- include('./header.ejs') %>
<%- include('./sidebar.ejs') %>
<div class="content-wrapper">
    <div class="card">
        <div class="card-header">
          <h3 class="card-title">Pichle Complaints</h3>
        </div>
        <div class="card-body">
          <table id="example1" class="table table-bordered table-striped">
            <thead>
            <tr>
              <th>Sr.No</th>
              <th>Job No</th>
              <th>Name</th>
              <th>Number</th>
              <th>Email</th>
              <th>Model</th>
              <th>Brand</th>
              <th>Device</th>
              <th>Problem</th>
              <th>Engineer</th>
              <th>Estimated</th>
              <th>Action</th>
            </tr>
            </thead>
            <tbody>
              <% i = 1 %>
              <% d.forEach((item) => { %>
                <tr>
                  <td><%= i++ %></td>
                  <td><%= item.jobNumber %></td>
                  <td><%= item.name %></td>
                  <td><%= item.phone %></td>
                  <td><%= item.email %></td>
                  <td><%= item.model %></td>
                  <td><%= item.brand %></td>
                  <td><%= item.device %></td>
                  <td><%= item.problem %></td>
                  <td><%= item.engineer %></td>
                  <td><%= item.estimated %></td>
                  <td>
                    <a href="/user/viewcomplaint/<%= item._id %>" class="btn btn-success">View</a>
                    <a href="/user/editcomplaint/<%= item._id %>" class="btn btn-info">Edit</a>
                    <a href="/user/deletecomplaint/<%= item._id %>" class="btn btn-danger">Delete</a>
                    <a href="/user/printcomplaint/<%= item._id %>" class="btn btn-danger">Print</a>
                  </td>
                </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
    </div>
</div>

<%- include('./footer.ejs') %>